{
  "name": "tradesman",
  "productName": "tradesman",
  "description": "Trade like you are gonna win it",
  "author": "Shubhranshu Tiwari",
  "version": "1.0.2",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/retah7/tradesman.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "ng serve --proxy-config proxy.json --ssl true -c local",
    "start:electron": "electron .",
    "build": "ng build",
    "build:prod": "ng build -c production",
    "build:electron": "ng build -c production --base-href ./ && electron .",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:build:mac": "rm -rf build-packages && electron-builder --mac --publish never",
    "electron:build:win64": "rm -rf build-packages && electron-builder --win --x64 --publish never",
    "electron:build:macwin": "electron-builder --mac --win --x64 --publish never",
    "electron:build:macwin:publish": "rm -rf dist && npm run build:prod && electron-builder --mac --win --x64 --ia32 --publish always",
    "postinstall": "install-app-deps",
    "pack": "electron-builder --dir",
    "package:electron": "electron-packager tradesman . --platform=win32 --arch=x64",
    "package-mac": "electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/windows/icon.ico --prune=true --out=release-builds --version-string.CompanyName='CE' --version-string.FileDescription='CE' --version-string.ProductName='ElectronTutorialApp'",
    "create-installer-mac": "electron-installer-dmg ./release-builds/tradesman-darwin-x64/tradesman.app tradesman --out=release-builds --overwrite "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.9",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.9",
    "@angular/compiler": "~8.2.9",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~8.2.9",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.9",
    "@angular/platform-browser-dynamic": "~8.2.9",
    "@angular/router": "~8.2.9",
    "@ng-bootstrap/ng-bootstrap": "~5.2.2",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "electron-log": "^4.2.1",
    "electron-updater": "^4.3.1",
    "font-awesome": "4.7.0",
    "hammerjs": "^2.0.8",
    "json2csv": "^5.0.1",
    "ng5-validation": "^1.0.0",
    "ngx-webcam": "^0.2.6",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.5.4",
    "store": "^2.0.12",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.8",
    "@angular/cli": "~8.3.8",
    "@angular/compiler-cli": "~8.2.9",
    "@angular/language-service": "~8.2.9",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "body-parser": "^1.19.0",
    "codelyzer": "^5.0.0",
    "electron": "^8.2.5",
    "electron-builder": "^21.2.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.2.1",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
